+static void optiga_shell_trim_cmd(char_t * user_cmd)
+{
+    char_t* i = user_cmd;
+    char_t* j = user_cmd;
+    while(*j != 0)
+    {
+        *i = *j++;
+        if(*i != ' ')
+            i++;
+    }
+    *i = 0;
+    if(strlen(user_cmd)>strlen("optiga --"))
+    {
+        strcpy(user_cmd,user_cmd+strlen("optiga --")-1);
+    }
+}
