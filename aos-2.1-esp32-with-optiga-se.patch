+    switch(type)
+    {
+        case ASYM_TYPE_RSA_SHA256_PKCS1: 
+        {
+            *signature_scheme = OPTIGA_RSASSA_PKCS1_V15_SHA256;
+        }
+        break;
+        case ASYM_TYPE_RSA_SHA384_PKCS1: 
+        {
+            *signature_scheme = OPTIGA_RSASSA_PKCS1_V15_SHA384;
+        }
+        break;
+        default :
+        {
+            return_status = IROT_ERROR_NOT_SUPPORTED;
+        }
+        break;
+    }
