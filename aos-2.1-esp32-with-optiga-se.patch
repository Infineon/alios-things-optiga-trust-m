+ * \brief Stops an os event.
+ *
+ * \details
+ * Stops an OS event.
+ *
+ * \pre
+ * - None
+ *
+ * \note
+ * - None
+ *
+ * \param[in] p_pal_os_event                Pointer to os event
+ *
+ */
+LIBRARY_EXPORTS void pal_os_event_stop(pal_os_event_t * p_pal_os_event);
+
+#ifdef __cplusplus
+}
