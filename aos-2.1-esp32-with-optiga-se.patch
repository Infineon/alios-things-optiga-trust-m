+                                                     &offset,
+                                                     AES_128_KEY_TAG,
+                                                     manifest_data->p_key_payload->key_params.aes_key.key_length,
+                                                     aes_key_info.key_length);
+        // Copy the payload formed
+        manifest_data->payload = aes_key;
