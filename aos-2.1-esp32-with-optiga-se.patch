+optiga_lib_status_t optiga_cmd_open_application(optiga_cmd_t * me, uint8_t cmd_param, void * params)
+{
+    OPTIGA_CMD_LOG_MESSAGE(__FUNCTION__);
+    optiga_cmd_execute(me,
+                       cmd_param,
+                       optiga_cmd_open_application_handler,
